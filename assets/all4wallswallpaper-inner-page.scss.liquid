/*-- Sticky Header ===============*/

.sticky_nav.main_nav_wrapper .main_nav .nav .menu {
  justify-content: center;
  padding: 12px 0;
}


/*-- Header ===============*/

.main_nav_wrapper .main_nav .nav .menu .vertical-menu li.sublink ul.vertical-menu_submenu li:first-child {
  padding-left: 24px;
}

.main_nav_wrapper {
  .main_nav {
    .logo {
      background-position: center;
      background-size: cover;
      background-repeat: no-repeat;
      line-height: 0;
    }
    .nav {
      .menu {
        margin: 0;
        max-width: 1616px;
        margin: 0 auto;
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 24px 20px;
        .vertical-menu {
          display: flex;
          li {
            &:first-child {
              padding-left: 0;
            }
            &:last-child {
              padding-right: 0;
            }
            a.dropdown_link--vertical {
              font-size: 17px;
              font-weight: 500;
              line-height: 27px;
            }
            a.top_link {
              font-size: 17px;
              font-weight: 500;
              line-height: 27px;
            }
          }
          .sublink {
            .icon-down-arrow {
              &:before {
                padding-left: 6px;
              }
            }
          }
        }
        li.search_container {
          margin: 0;
          padding: 0 0 0 52px;
          .search_form {
            input {
              max-width: 282px;
              min-height: 52px;
              background: #fff;
              color: #000;
              max-width: 282px;
              min-height: 52px;
              background: #fff;
              color: #000;
              font-size: 16px;
              font-weight: 300;
              padding-left: 20px;
              &::placeholder {
                color: #000;
                opacity: 0.5;
              }
            }
            .icon-search {
              top: 17px;
              right: 14px;
              &:before {
                opacity: 0.5;
                color: #000000;
              }
            }
          }
        }
        &:after {
          content: none;
        }
      }
    }
  }
}

.header-section {
  .header {
    .top_bar {
      background-color: #303030;
      .promo-text-center {
        position: absolute;
        color: #fff;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        .promo_banner__content {
          p {
            margin: 0;
            font-size: 14px;
            font-weight: 500;
            letter-spacing: 0.64px;
          }
        }
      }
      .cart_container {
        a.mini_cart {
          max-width: 132px;
          width: 100%;
          display: inline-block;
          margin: 0;
          padding: 0;
        }
      }
      .menu.right {
        a.icon-user {
          padding: 0;
          padding-right: 16px;
          &:before {
            font-size: 16px;
            vertical-align: middle;
            margin: 0;
            padding-right: 8px;
          }
          span {
            letter-spacing: 0.14px;
            text-transform: capitalize;
            padding: 0;
            vertical-align: middle;
          }
        }
      }
    }
  }
}


/*-- Featured-promotions ===============*/
.featured-promotions-section {
  .featured-promotions .feature-section:hover .feature-overlay{
    background:transparent;
  }
  .featured-promotions {
    padding-bottom: 0;
    .feature-section {
      padding: 11px 6px;

      .feature-inner {
        display: block;
        position: unset;
        overflow: hidden;
        transition: transform 0.6s;
        transform-style: preserve-3d;
        position: relative;
        img{
          width: 100%; 
          height: 100%; 
          backface-visibility: hidden;
          top: 0;
        }
        .feature-overlay {
          .feature-wrap {
            padding: 0;
            top: 88%;
            text-align: center;
            padding-left:0;
            h2.title {
              font-family: Poppins;
              font-size: 54px;
              font-weight: bold;
              margin: 0;
              &:after {
                content: none;
              }
            }
          }
        }
      }
    }
    .feature-section .secondary-hover{
      position: absolute;
      top: 0;
      transform: rotateY(180deg);
    }
    .feature-section:hover .feature-overlay {
    /*transform: rotateY(180deg);*/
  }
    .feature-section:hover .secondary-hover{
      /*display:block;*/
    }
  }
}


#shopify-section-1572400078523 figure.feature-section.title-overlay-true.promo-animation--true.animated.fadeInDown.dm-hovered{
  .feature-inner {
    transform: rotateY(180deg);
  }
  .secondary-hover {
    backface-visibility: visible;
  }
  h2{
    transform: rotateY(180deg);
  }
}


